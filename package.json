{
  "name": "ai-sheets-analyst",
  "version": "1.0.0",
  "description": "AI-powered Google Sheets analyst with multi-user collaboration",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "supabase functions serve --env-file .env.local",
    "build": "pnpm -r build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:phase0": "vitest run --config vitest.config.integration.ts --reporter verbose",
    "test:supabase": "deno test --allow-env --allow-net --allow-read supabase/functions/tests",
    "lint": "pnpm -r lint",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "deploy:functions": "supabase functions deploy --project-ref $SUPABASE_PROJECT_REF",
    "migrate:dev": "supabase db reset --local",
    "migrate:prod": "supabase db push --project-ref $SUPABASE_PROJECT_REF",
    "clasp:push": "cd apps/addon && npx clasp push",
    "clasp:pull": "cd apps/addon && npx clasp pull"
  },
  "devDependencies": {
    "@google/clasp": "^3.1.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^4.0.4",
    "dotenv": "^17.2.3",
    "eslint": "^8.0.0",
    "googleapis": "^164.1.0",
    "prettier": "^3.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vitest": "^4.0.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
